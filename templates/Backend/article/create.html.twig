{% extends 'base.html.twig' %}

{% block body %}
    <h1>Write an article!</h1>
    <div class="form-article">
        {{ form_start(form) }} 
            {{ form_errors(form) }} 
            {{ form_row(form.title) }} 
            {{ form_widget(form.categories) }}
            {{ form_row(form.content) }} 
            {{ form_row(form.validate, { 'label': 'Create' }) }} 
        {{ form_end(form) }} 
    </div>
{% endblock %}

